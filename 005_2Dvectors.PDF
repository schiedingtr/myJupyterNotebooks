    "import numpy as np\n",
    "import math as m\n",
    "a = np.array([1, -2, 16, 40, 0])\n",
    "b = np.array([3.16, 25, -3, -6.5, 108])\n",
    "print('c=', a - b)\n",
    "d = m.sqrt((-2.16)**2 + (-27)**2 + 19**2 + 46.5**2 + 108**2)\n",
    "print('length of c=', d)\n",
    "from numpy.linalg import norm\n",
    "print('Norm A=',norm(a, ord=2))\n",
    "print('Norm B=',norm(b, ord=2))\n",
    "B = norm(b, ord=2)\n",
    "A = norm(a, ord=2)\n",
    "print('A-B', A-B)\n",
    "print('a * b', np.dot(a, b, out = None))\n",
    "\n"
   ]
